<form method="post" action="{{ path('reservation_delete', {'id': reservationlist.id}) }}" onsubmit="return confirm('Voulez-vous vraiment annuler la reservation ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ reservationlist.id) }}">
    <button class="btn red darken-2"><i class="material-icons left">clear</i>Annuler</button>
</form>
